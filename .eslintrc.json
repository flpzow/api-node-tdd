{
    "env": {
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "no-use-before-define": "off",
        "prettier/prettier": "error"
    }
}
